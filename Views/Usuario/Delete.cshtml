@{ 
    ViewData["Title"] = "Eliminar Usuario";
}

@model Usuario

<h1>@ViewData["Title"]</h1>
<form asp-controller="Usuario" asp-action="Delete">
    <div class="form-group">
        <label asp-for="@Model.id_usuario">ID del usuario a Eliminar</label>
        <input type="text" asp-for="@Model.id_usuario" class="form-control" id="nuevaId">  
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>